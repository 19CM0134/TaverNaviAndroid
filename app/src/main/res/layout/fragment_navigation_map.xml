<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="630dp"
    android:orientation="vertical"
    android:background="@color/HalfCream"
    tools:context=".ui.defaultmap.MapFragment" >

    <TextView
        android:id="@+id/map_restaurant_name"
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:background="@color/DarkSienna"
        android:padding="12dp"
        android:textColor="@color/Cultured"
        android:textSize="20sp"
        android:textStyle="bold"
        android:text="TextView" />

    <fragment
        android:id="@+id/navMapFragment"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>